{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "sendme",
  "title": "Sendme File Share",
  "description": "Share files using sendme with ease",
  "icon": "command-icon.png",
  "author": "your-name",
  "owner": "nazif",
  "categories": ["Developer Tools", "Productivity"],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Share File",
      "description": "Share File",
      "mode": "view",
      "icon": "command-icon.png"
    },
    {
      "name": "receive",
      "title": "Receive File",
      "description": "Download a file by pasting a sendme ticket",
      "mode": "view",
      "icon": "command-icon.png"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.55.2",
    "@raycast/utils": "^1.10.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "~18.16.0",
    "@types/react": "^18.0.28",
    "eslint": "^7.32.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}